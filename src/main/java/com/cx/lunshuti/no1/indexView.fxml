<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cx.lunshuti.no1.IndexController">
    <MenuBar layoutX="20.0" layoutY="20.0">
        <Menu text="文件">
            <MenuItem fx:id="openFile" text="打开文件"/>
        </Menu>
        <Menu text="编辑">
            <MenuItem text="复制"/>
            <MenuItem text="粘贴"/>
        </Menu>
    </MenuBar>
    <Rectangle fx:id="rectangle" fill="#c6c6c6" height="200.0" layoutX="50.0"
               layoutY="50.0" width="400.0"/>
    <Button fx:id="blue" layoutX="50.0" layoutY="300.0" text="蓝色字体"/>
    <Button fx:id="yellow" layoutX="350.0" layoutY="350.0" text="黄色字体"/>
    <Label fx:id="text" layoutX="200.0" layoutY="200.0"/>
</AnchorPane>
